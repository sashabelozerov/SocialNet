<html>
<head>
<title><%= SocialNet %></title>
<%= stylesheet_link_tag 'blueprint/screen', :media => 'screen' %>
<%= stylesheet_link_tag 'blueprint/print',  :media => 'print' %>
</head>
<body>

<% if !current_user %>
  <%= link_to "Register", new_user_url %> |
  <%= link_to "Log In", login_url %>
<% else %>
  <%= link_to "My Account", user_url(@current_user) %> |
  <%= link_to "Logout", logout_url, :confirm => "Are you sure you want to logout?" %>
<% end %>
<br />
<% if flash[:error] %>
      <%= flash[:error] %>
<% end %>
<br />
<% if flash[:notice] %>
      <%= flash[:notice] %>
 <% end %>

<%= yield  %>
</body>
</html>
